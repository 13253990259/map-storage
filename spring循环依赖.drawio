        <mxCell id="xUoOStjw7muQhoDG1cdU-86" value="问题2，为啥要放FactoryBean" style="text;strokeColor=none;fillColor=none;html=1;fontSize=24;fontStyle=1;verticalAlign=middle;align=center;fontColor=#7EA6E0;" vertex="1" parent="1">
